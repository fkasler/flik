{
    "{{ hc_domain }}": {
        "primary_target": "{{ primary_target }}",
        "secondary_target": "{{ secondary_target }}",
        "search_string": "{{ search_string }}",
        "wwwroot": "{{ wwwroot }}",
        "tracking_cookie": "cla_cookie",
        "replacements": {
            "string_to_be_replaced": "replacement_string"
        },
        "custom_headers": {},
